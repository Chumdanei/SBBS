<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
 	<class name="com.EntityClasses.Booking_Request_Master" table="booking_request_master">
 	 	
 	 <id name="id" column="id" >    
 	    <generator class="increment"/>    
 	 </id>
    <property name="destination_id" column="destination_id"/>
    <property name="source_id" column="source_id"/>
    <property name="dept_date" column="dept_date"/>
    <property name="dept_time" column="dept_time"/>
    <property name="from_id" column="from_id"/>
    <property name="to_id" column="to_id"/>
    <property name="number_of_booking" column="number_of_booking"/>
    <property name="adult" column="adult"/>
    <property name="child" column="child"/>
    <property name="total_cost" column="total_cost"/>
    <property name="description" column="description"/>
    <property name="user_id"  column="user_id"/>
    <property name="provided_time"  column="provided_time"/>
    <property name="status" column="status"/>
    <property name="created_at" column="created_at"/>
    <property name="updated_at" column="updated_at"/>
    <property name="enabled" type="java.lang.Boolean">
            <column name="enabled" not-null="true" />
     </property>
 	</class>
</hibernate-mapping>